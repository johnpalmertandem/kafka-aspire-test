{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "messaging": ""
  },  
  "Aspire": {
    "Confluent": {
      "Kafka": {
        "Consumer": {
          "Config": {
            "AutoOffsetReset": "Earliest",
            "GroupId": "aspire",
            "ClientId": "123"
          }
        }
      }
    },
    "Npgsql": {
      "EntityFrameworkCore": {
        "PostgreSQL": {
          "ConnectionString": "YOUR_CONNECTIONSTRING",
          "DbContextPooling": true,
          "DisableHealthChecks": false,
          "DisableTracing": false
        }
      }
    }
  }
}
